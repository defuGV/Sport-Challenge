        <applies_extended>false</applies_extended>
        <condition/>
        <description>Field state on the sport challenge table is colored and highlighted based on the current state:&#13;
New - without color&#13;
In progress - yellow&#13;
Completed - green&#13;
Not reached - red</description>
        <field>state</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>GVS_SPORT Challenge states field colors</name>
        <order/>
        <script><![CDATA[// field coloring
function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (newValue === '') {
        return;
    }

    if (newValue === 'completed') {
        control.style.backgroundColor = '#90ee90';
        return;
    }

    if (newValue === 'in_progress') {
        control.style.backgroundColor = 'yellow';
        return;
    }

    if (newValue === 'not_reached') {
        control.style.backgroundColor = '#FF6347';
        return;
    }

    control.style.backgroundColor = '';
	}]]></script>
